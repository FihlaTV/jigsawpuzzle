{
	"name": "JigSawPuzzle",
	"description": "A jigsaw puzzle",
	"version": "1.4.1.0",
	"manifest_version": 2,
	"homepage_url": "http://josemoya.blogspot.com",
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"unlimitedStorage"
	],
	"offline_enabled": true
}